<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id='purchase_order_kpis_tree_inherit_knk' model="ir.ui.view">
        <field name="name">purchase.order.kpis.tree.inherit.knk</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_kpis_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='origin']" position="after">
                <field name="delivery_detail"/>
            </xpath>
        </field>
    </record>
    <record id='product_supplierinfo_tree_view2_inherit_mdpn' model="ir.ui.view">
        <field name="name">product.supplierinfo.tree.view2.inherit.mdpn</field>
        <field name="model">product.supplierinfo</field>
        <field name="inherit_id" ref="purchase.product_supplierinfo_tree_view2"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="attributes">
                <attribute name="editable">top</attribute>
            </xpath>
            <xpath expr="//field[@name='date_start']" position="attributes">
                <attribute name="optional">Show</attribute>
                <attribute name="string">Apply Date</attribute>
            </xpath>
            <xpath expr="//field[@name='date_end']" position="replace">
            </xpath>
        </field>
    </record>
</odoo>
