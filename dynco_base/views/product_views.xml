            </field>
        </record>

        <record model="ir.ui.view" id="view_product_icon_form">
            <field name="name">product.icon.form</field>
            <field name="model">product.icon</field>
            <field name="arch" type="xml">
                <form string="Product Icons">
                    <sheet>
                        <field name="icon" widget="image" class="oe_avatar" options="{'size': [90, 90]}"/>
                        <div class="oe_title">
                            <label class="oe_edit_only" for="name" string="Icon Name"/>
                            <h1><field name="name" placeholder="Icon Name" required="True"/></h1>
                            <field name="product_tmpl_id" invisible="1"/>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="view_product_customer_code_form">
            <field name="name">view.product.customer.code.form</field>
            <field name="model">product.customer.code</field>
            <field name="arch" type="xml">
                <form string="Product Customer Code">
                    <sheet>
                        <group>
                            <group>
                                <field name="product_code"/>
                            </group>
                            <group>
                                <field name="partner_id"/>
                                <field name="product_tmpl_id"
                                       string="Product"
                                       invisible="context.get('visible_product_tmpl_id', True)"/>
                                <field name="product_id"
                                       domain="[('product_tmpl_id', '=', product_tmpl_id)]"
                                       invisible="1"/>
                                <field name="company_id"
                                       options="{'no_create': True}"
                                       groups="base.group_multi_company"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="view_product_customer_code_tree">
            <field name="name">view.product.customer.code.tree</field>
            <field name="model">product.customer.code</field>
            <field name="arch" type="xml">
                <tree string="Product Customer Code">
                    <field name="sequence" widget="handle"/>
                    <field name="product_code"/>
                    <field name="partner_id"/>
                    <field name="company_id"
                           groups="base.group_multi_company"
                           options="{'no_create': True}"/>
                    <field name="product_tmpl_id"
                           string="Product"/>
                    <field name="product_id"
                           invisible="1"/>
                </tree>
            </field>
        </record>

        <record id="action_product_customer_code_tree" model="ir.actions.act_window">
            <field name="name">Product Customer Code</field>
            <field name="res_model">product.customer.code</field>
            <field name="view_id" ref="view_product_customer_code_tree"/>
        </record>

        <menuitem action="action_product_customer_code_tree"
                   id="menu_product_customer_code"
                   sequence="6"
                   parent="sale.prod_config_main"/>

        <record model="ir.ui.view" id="view_product_supplierinfo_form_custom">
            <field name="name">product.supplierinfo.form.view.custom</field>
            <field name="model">product.supplierinfo</field>
            <field name="inherit_id" ref="product.product_supplierinfo_form_view" />
            <field name="priority">99</field>
