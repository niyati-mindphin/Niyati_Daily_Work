                        <field name="cost_price" class="oe_inline" /><field name="cost_currency_id" class="oe_inline"/>
                    </div>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="view_product_supplierinfo_tree_custom">
            <field name="name">product.supplierinfo.tree.view.custom</field>
            <field name="model">product.supplierinfo</field>
            <field name="inherit_id" ref="product.product_supplierinfo_tree_view" />
            <field name="priority">99</field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='price']" position="replace">
                    <field name="price" string="Purchase"/>
                </xpath>
                <xpath expr="//field[@name='price']" position="after">
                    <field name="cost_price" />
                </xpath>
                <xpath expr="//field[@name='product_tmpl_id']" position="before">
                    <field name="default_code" string="Art-Nr"/>
                </xpath>
                <!-- <xpath expr="//tree" position="attributes">
                    <attribute name="default_order">id desc</attribute>
                </xpath> -->
