        <menuitem id="knk_main_menu_products_app" name="Products" sequence="11" web_icon="dynco_base,static/description/product_menu_icon.png"/>

        <menuitem id="knk_menu_product_templ" parent="knk_main_menu_products_app" action="product.product_template_action" sequence="1"/>

        <menuitem id="knkmenu_product_product" parent="knk_main_menu_products_app" action="product.product_normal_action_sell" sequence="2"/>

        <record id="product_form_view_knk_cust" model="ir.ui.view">
            <field name="name">product.form.view.knk</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='action_view_sales']" position="replace">
                    <button class="oe_stat_button" name="action_view_sales"
                        type="object" icon="fa-signal" groups="sales_team.group_sale_salesman" help="Sold in the last 365 days">
                        <div class="o_field_widget o_stat_info">
                            <span class="o_stat_value">
                                <field name="sales_count_cnt" widget="statinfo" nolabel="1" class="mr4"/>
                                <field name="uom_name"/>
                            </span>
                            <span class="o_stat_text">Sold</span>
                        </div>
                    </button>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="product_template_form_view_knk_cust">
            <field name="name">product.template.form.view.knk</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="sale.product_template_form_view_sale_order_button"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='action_view_sales']" position="replace">
                    <button class="oe_stat_button" name="action_view_sales"
                        type="object" icon="fa-signal" groups="sales_team.group_sale_salesman" help="Sold in the last 365 days">
                        <div class="o_field_widget o_stat_info">
                            <span class="o_stat_value">
                                <field name="sales_count_cnt" widget="statinfo" nolabel="1" class="mr4"/>
                                <field name="uom_name"/>
                            </span>
                            <span class="o_stat_text">Sold</span>
                        </div>
                    </button>
                </xpath>
            </field>
        </record>

        <record id="view_product_image_form_extra_img_url_inherit" model="ir.ui.view">
            <field name="name">product.image.form.extra.image.url.inherit</field>
            <field name="model">product.image</field>
            <field name="inherit_id" ref="website_sale.view_product_image_form"/>
            <field name="arch" type="xml">
                <field name="video_url" position="after">
                    <label for="product_extra_img_url"/>
                    <field name="product_extra_img_url" widget="url"/>
                </field>
            </field>
        </record>

        <record id="product_template_kanban_stock_view" model="ir.ui.view">
            <field name="name">Product Template Kanban Stock Inherit</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="stock.product_template_kanban_stock_view"/>
            <field name="arch" type="xml">
                <field name="type" position="after">
                    <field name="curr_location"/>
                    <field name="show_on_hand_qty_status_button"/>
                </field>
                <xpath expr="//field[@name='qty_available']//.." position="replace">
                    <div t-if="record.show_on_hand_qty_status_button.raw_value">
                        <t t-raw="record.curr_location.value"/> 
                    </div>
                </xpath>
            </field>
        </record>
        <record model="ir.ui.view" id="view_assemble_product_price_purchase">
            <field name="name">product.template.form.view.custom</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="purchase.view_product_supplier_inherit" />
            <field name="priority">99</field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='seller_ids']" position="before">
                    <field name="bom_ids" invisible="1"/>
                    <button name="action_assemble_product_price" string="Compute BOM Cost" type="object" class="btn-primary" attrs="{'invisible': [('bom_ids', '&lt;=', 0)]}"/>
                </xpath>
            </field>
        </record>
        <record id="product_pricelist_view_inherit_ecc_categ" model="ir.ui.view">
            <field name="name">product.pricelist.form.inherit.ecc.categ</field>
            <field name="model">product.pricelist</field>
            <field name="inherit_id" ref="website_sale.website_sale_pricelist_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='pricelist_config']//group[@name='pricelist_website']" position="inside">
                    <field name="ecommerce_category"/>
                </xpath>
            </field>
        </record>
        <record id="product_pricelist_item_form_view_inherit" model="ir.ui.view">
            <field name="name">product.pricelist.item.form.inherit</field>
            <field name="model">product.pricelist.item</field>
            <field name="inherit_id" ref="product.product_pricelist_item_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='date_end']/.." position="after">
                    <field name="cm_label_id"/>
                    <field name="show_discount_on_label" attrs="{'invisible': [('applied_on', 'not in', ['1_product', '0_product_variant'])]}"/>
                </xpath>
            </field>
        </record>
        <record id="product_item_2_inherit_label_disc" model="ir.ui.view">
            <field name="name">Theme Prime Product Item 2 - Label Disc</field>
            <field name="inherit_id" search="[('key', '=', 'theme_prime.product_item_2'), ('website_id', '=', 20)]"/>
            <field name="website_id" search="[('id', '=', 20)]"/>
            <field name="type">qweb</field>
            <field name="priority">20</field>
            <field name="key">dynco_base.product_item_2</field>
            <field name="arch" type="xml">
                <xpath expr="//t[@t-call='theme_prime.product_label']" position="replace">
                    <t t-set="price_rule" t-value="pricelist.get_product_price_rule(product, 1, request.env.user.partner_id)[1]"/>
                    <t t-set="disc" t-value="False" />
                    <t t-if="price_rule">
                        <t t-set="price_rule" t-value="request.env['product.pricelist.item'].browse(price_rule)"/>
                        <t t-if="price_rule and price_rule.cm_label_id">
                            <t t-if="price_rule.show_discount_on_label">
                                <t t-set="disc" t-value="price_rule.discountPercentage()" />
                            </t>
                            <t t-set="label" t-value="price_rule.cm_label_id"/>
                        </t>
                    </t>
                    <t t-if="disc and int(disc.replace('%', '')) > 0">
                        <t t-call="theme_prime.product_label">
                            <t t-set="disc" t-value="disc"/>
                            <t t-set="label" t-value="label"/>
                        </t>
                    </t>
                </xpath>
                <xpath expr="//form" position="inside">
                    <t t-set="availability_check"  t-value="website.get_product_stock_available(product)" />
                    <t t-if="availability_check.get('is_out_of_stock') and not product.allow_out_of_stock_order and not product.sudo().avail_only_specific_countries">
                        <span class="o_ribbon bg-danger o_ribbon_left" style="">OUT OF STOCK</span>
                    </t>
                    <t t-if="product.sudo().avail_only_specific_countries">
                        <t t-if="product.check_available_country()">
                            <t t-if="product.sudo().stock_availability_for_warehouse">
                                <t t-set="availability_check" t-value="website.check_add_to_cart_product_stock(product)" />
                                <t t-set="available_threshold" t-value="website.check_available_threshold(product)" />

                                <t t-if="available_threshold.get('available_threshold')">
                                </t>
                                <t t-elif="available_threshold.get('prevent_inventory_out')">
                                    <span class="o_ribbon bg-danger o_ribbon_left" style="">OUT OF STOCK</span>
                                </t>
                                <t t-elif="available_threshold.get('prevent_inventory_in')">
                                </t>
                                <t t-else="">
                                    <t t-if="availability_check.get('is_out_of_stock')">
                                        <span class="o_ribbon bg-danger o_ribbon_left" style="">OUT OF STOCK</span>
                                    </t>
                                </t>
                            </t>
                        </t>
                        <t t-else="">
                            <span class="o_ribbon bg-danger o_ribbon_left not_avail_in_you_country" style="">NOT AVAILABLE</span>
                        </t>
                    </t>
                </xpath>
            </field>
        </record>
        <record id="product_item_2_inherit_label_disc_pelletray" model="ir.ui.view">
            <field name="name">Theme Prime Product Item 2 - Label Disc pelletray</field>
            <field name="inherit_id" search="[('key', '=', 'theme_prime.product_item_2'), ('website_id', '=', 24)]"/>
            <field name="website_id" search="[('id', '=', 24)]"/>
            <field name="type">qweb</field>
            <field name="priority">20</field>
            <field name="key">dynco_base.product_item_2</field>
            <field name="arch" type="xml">
                <xpath expr="//form" position="inside">
                    <t t-set="availability_check"  t-value="website.get_product_stock_available(product)" />
                    <t t-if="availability_check.get('is_out_of_stock') and not product.allow_out_of_stock_order and not product.sudo().avail_only_specific_countries">
                        <span class="o_ribbon bg-danger o_ribbon_left" style="">OUT OF STOCK</span>
                    </t>
                    <t t-if="product.sudo().avail_only_specific_countries">
                        <t t-if="product.check_available_country()">
                            <t t-if="product.sudo().stock_availability_for_warehouse">
                                <t t-set="availability_check" t-value="website.check_add_to_cart_product_stock(product)" />
                                <t t-set="available_threshold" t-value="website.check_available_threshold(product)" />

                                <t t-if="available_threshold.get('available_threshold')">
                                </t>
                                <t t-elif="available_threshold.get('prevent_inventory_out')">
                                    <span class="o_ribbon bg-danger o_ribbon_left" style="">OUT OF STOCK</span>
                                </t>
                                <t t-elif="available_threshold.get('prevent_inventory_in')">
                                </t>
                                <t t-else="">
                                    <t t-if="availability_check.get('is_out_of_stock')">
