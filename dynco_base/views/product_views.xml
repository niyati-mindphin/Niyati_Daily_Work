<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <data>
        <record id="product_category_inherit_view_bottom_descript" model="ir.ui.view">
            <field name="name">product.public.category.bottom.descript.form</field>
            <field name="model">product.public.category</field>
            <field name="inherit_id" ref="website_sale.product_public_category_form_view"></field>
            <field name="arch" type="xml">
                <xpath expr="//div" position="after">
                    <notebook>
                        <page string="Category Description">
                            <field name="website_description" nolabel="1"/>
                        </page>
                        <page string="Meta Info">
                            <group>
                                <field name="website_meta_title" string="Meta Title"/>
                                <field name="website_meta_keywords" string="Meta Keyword"/>
                                <field name="website_meta_description" string="Meta Description"/>
                                <field name="product_tmpl_ids">
                                    <tree>
                                        <field name="sequence" widget="handle"/>
                                        <field name="name" />
                                        <field name="default_code" />
                                        <field name="barcode" />
                                    </tree>
                                </field>
                            </group>
                        </page>
                        <page string="Category Bottom Description">
                            <field name="categ_bottom_description" nolabel="1"/>
                        </page>
                    </notebook>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="product_video_form_vew_spt">
            <field name="name">product.video.form.vew.spt</field>
            <field name="model">product.video</field>
            <field name="arch" type="xml">
                <form string="Product Videos">
                    <field name="sequence" invisible="1"/>
                    <div class="row o_website_sale_image_modal">
                        <div class="col-md-6 col-xl-5">
                            <label for="name" string="Image Name"/>
                            <h2><field name="name" placeholder="Image Name"/></h2>
                            <label for="video_url" string="Video URL"/><br/>
                            <field name="video_url"/><br/>
                            <field name="image" widget="image" invisible="1"/><br/>
                        </div>
                        <div class="col-md-6 col-xl-7 text-center o_website_sale_image_modal_container">
                            <div class="row">
                                <div class="col-6" attrs="{'invisible': [('video_url', 'in', ['', False])]}">
                                    <div class="o_video_container p-2">
                                        <span>Video Preview</span>
                                        <field name="embed_code" class="mt-2" widget="video_preview"/>
                                        <h4 class="o_invalid_warning text-muted text-center" attrs="{'invisible': [('embed_code', '!=', False)]}">
                                            Please enter a valid Video URL.
                                        </h4>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </field>
        </record>

        <record id="product_template_form_view_product_customer_cod" model="ir.ui.view">
            <field name="name">product.template.form.customer.code</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='product_template_images']" position="before">
                    <group class="col-12"> 
                        <field name="media_visibility" widget="radio" style="display: -webkit-inline-box!important;"/> 
                    </group>
                    <group>
                    </group>
                </xpath>
                <xpath expr="//group[@name='product_template_images']" position="after">

                    <group name="extra_images_copyright" string="Extra Images With Copyright" attrs="{'invisible': [('sale_ok','=',False)]}">
                        <field name="cproduct_image_ids" class="o_website_sale_image_list" context="{'default_name': name}" mode="kanban" add-label="Add a Media" nolabel="1"/>
                    </group>
                </xpath>
                <page name="sales" position="after">
                    <page string="Customer Code">
                         <field name="product_customer_code_ids"
                                context="{'default_product_tmpl_id': context.get('product_tmpl_id', active_id)}"
                                widget="one2many_list">
                            <tree string="Product Customer Code" editable="bottom">
                                <field name="sequence" widget="handle"/>
                                <field name="product_code"/>
                                <field name="partner_id"/>
                                <field name="company_id"
                                       groups="base.group_multi_company"
                                       options="{'no_create': True}"/>
                                <field name="product_tmpl_id"
                                       string="Product"
                                       invisible="context.get('visible_product_tmpl_id', True)"/>
                                <field name="product_id"
                                       invisible="1"/>
                            </tree>
                            <form string="Product Customer Code">
                                <group>
                                    <group>
                                        <field name="product_code"/>
                                    </group>
                                    <group>
                                        <field name="partner_id"/>
                                        <field name="product_tmpl_id"
                                               string="Product"
                                               invisible="context.get('visible_product_tmpl_id', True)"/>
                                        <field name="product_id"
                                               domain="[('product_tmpl_id', '=', product_tmpl_id)]"
                                               invisible="1"/>
                                        <field name="company_id"
                                               options="{'no_create': True}"
                                               groups="base.group_multi_company"/>
                                    </group>
                                </group>
                            </form>
                        </field>
                    </page>
                </page>
                <xpath expr="//label[@for='standard_price']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//div[@name='standard_price_uom']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
            </field>
        </record>

        <record id="product_template_form_view_product_sale_delay" model="ir.ui.view">
            <field name="name">product.template.form.sale.delay</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="stock.view_template_property_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='operations']/div" position="after">
                  <label for="sale_delay_b2b" attrs="{'invisible': [('sale_ok', '=', False)]}"/>
                        <div attrs="{'invisible': [('sale_ok', '=', False)]}">
                            <field name="sale_delay_b2b" class="oe_inline" style="vertical-align:baseline"/> days
                        </div>
                </xpath> 
              </field>
        </record>

